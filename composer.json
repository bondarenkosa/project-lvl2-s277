{
    "name": "bondsa/gendiff",
    "bin": [
        "bin/gendiff"
    ],
    "description": "Generate diff between two documents (json, yaml, ini)",
    "type": "project",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Sergei Bondarenko",
            "email": "andbondsa@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Gendiff\\": "src",
            "Gendiff\\Tests\\": "tests"
        },
        "files": [
            "src/Cli.php",
            "src/Lib.php"
        ]
    },
    "scripts": {
        "phpcs": "phpcs",
        "phpunit": "phpunit",
        "test": "phpunit tests"
    },
    "require": {
        "docopt/docopt": "^1.0",
        "symfony/yaml": "^4.1"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.3",
        "codeclimate/php-test-reporter": "^0.4.4",
        "phpunit/phpunit": "^7.2",
        "mikey179/vfsStream": "^1.6"
    }
}
